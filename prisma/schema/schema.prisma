generator client {
  provider = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  USER
  MANAGER
  CLUB_OWNER
}

enum InviteStatus {
  PENDING
  ACCEPTED
  REJECTED
  EXPIRED
  REVOKED
}

enum ReviewType {
  USER
  CLUB
  EVENT
}
